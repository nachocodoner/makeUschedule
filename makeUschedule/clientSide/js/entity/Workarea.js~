(function(se){
	se.Entities.Workarea = new Class({
		initialize: function(idWorkarea, schedule){
			this.id = idWorkarea;
			
			var workarea = new Element('div',{
				id: idWorkarea
			});

			this.menu = new Element('div', {
				'class': 'menu'
			});

			this.tasksMenu = new Element('div', {
				'class': 'tasksMenu'
			});

			this.display = new Element('div', {
				'class': 'display'
			});
			
			this.menu.inject(workarea);
			this.tasksMenu.inject(this.menu);
			this.display.inject(workarea);

			workarea.inject(document.body, 'top');
			
			this.element = workarea;
			this.schedule = schedule;
		}
	});
})(se);
